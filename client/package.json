{
  "name": "babysitter-finder-client",
  "version": "1.0.0",
  "description": "Frontend for Babysitter Finder application - Vanilla HTML/JS SPA",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=8080 --host=localhost --open=/index.html",
    "dev": "live-server --port=8080 --host=localhost --open=/index.html --watch=.",
    "build": "echo 'Static HTML/JS build - copy files to production server'",
    "serve": "http-server -p 8080 -c-1"
  },
  "keywords": [
    "vanilla-js",
    "spa",
    "babysitter",
    "finder",
    "frontend"
  ],
  "author": "Babysitter Finder Team",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.1"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "http-server": "^14.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
} 